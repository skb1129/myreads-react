{
  "name": "myreads-react",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-icons": "^3.1.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
